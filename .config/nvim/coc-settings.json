{
  "coc.preferences.messageLevel": "warning",
  "coc.preferences.bracketEnterImprove": false,
  "diagnostic.displayByAle": true,
  "suggest.noselect": false,
  "suggest.maxCompleteItemCount": 10,
  "python.jediEnabled": false,
  "javascript.validate.enable": false,
  "solargraph.checkGemVersion": false,
  "list.insertMappings": {
    "<C-v>": "action:vsplit",
    "<C-s>": "action:split"
  },
  "languageserver": {
    "golang": {
      "command": "~/go/bin/gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  }
}
