	function(${1}) {
		${0}
	}

snippet     for
options     head
	for (var ${1:i}; $1 < ${2:#:arr}.length; $1++) {
		${0}
	}

snippet     fori
options     head
	for (var ${1:i} in ${2:#:arr}) {
		${0}
	}

