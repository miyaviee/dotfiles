snippet vd
	var_dump(${0});

snippet ex
	exit;

snippet fore
	foreach ($${1:#:variable} as $key => $value) {
		${0:TARGET}
	}

snippet if
	if (${1:#:cond}) {
		${0:TARGET}
	}

snippet die
	die();

snippet cla
	class ${1:#:class_name}
	{
		${0:TARGET}
	}

snippet fun
	function ${1:#:method name}(${2:#:variable})
	{
		${0:TARGET}
	}

snippet doc
	/**
	 *
	 * ${0:TARGET}
	 *
	 */


