snippet vd
	var_dump(${0});exit;

snippet fore
	foreach ($${1:#:variable} as $key => $value) {
		${0:TARGET};
	}

snippet if
	if (${1:#:cond}) {
		${0:TARGET};
	}

snippet die
	die();

snippet cla
	class ${1:#:class_name}
	{
		${0:TARGET}
	}

snippet func
	function ${1:#:method name}(${2:#:variable})
	{
		${0:TARGET};
	}

snippet arr
	array(${0:TARGET});

snippet doc
	/**
	 *
	 * ${1:#:comment}
	 * @param  ${2:#:type}
	 * @return ${0:#:type}
	 *
	 */
